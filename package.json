{
  "name": "s3-pgp-encryptor-lib",
  "version": "0.1.0",
  "description": "Lambda function that encrypts files uploaded to S3.",
  "main": "dist/app.js",
  "scripts": {
    "build-ts": "tsc",
    "encrypt-it": "ts-node encryptIt.ts",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "build": "npm run lint && npm run build-ts",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "18.x.x",
    "npm": "8.x.x"
  },
  "author": "Jeremy Epstein",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Jaza/s3-pgp-encryptor-lib",
  "keywords": [
    "pgp",
    "s3",
    "encryption"
  ],
  "repository": "github:Jaza/s3-pgp-encryptor-lib",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "aws-sdk-client-mock": "^2.1.1",
    "aws-sdk-client-mock-jest": "^2.1.1",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.342.0",
    "@aws-sdk/client-secrets-manager": "^3.342.0",
    "dotenv": "^16.1.1",
    "dotenv-expand": "^10.0.0",
    "openpgp": "^5.9.0",
    "winston": "^3.9.0"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/tests/**/*.+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
